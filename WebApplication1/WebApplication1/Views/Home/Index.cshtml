@model HomeVM;



@using (Html.BeginForm("Logout", "Home", FormMethod.Post))
{
    <p>Salam necesen</p>
    <p>@Model.Group.Name</p>
    <hr />

    @foreach (var item in Model.Students)
    {
        <p>@item.Name</p>
        <p>@item.Age</p>
        <p>@item.GroupId</p>
        <hr />
    }

    <button type="submit">LogOut</button>
}